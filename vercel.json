{
  "version": 2,
  "builds": [
    {
      "src": "*.py",
      "use": "@vercel/python",
      "config": { 
        "runtime": "python3.12",
        "installCommand": "pip install --no-cache-dir --prefer-binary -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/$1"
    }
  ]
}
